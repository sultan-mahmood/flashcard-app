{
  "name": "braindash",
  "version": "1.0.0",
  "main": "expo-router/entry",
  "homepage": "https://sultan-mahmood.github.io/flashcard-app",
  "scripts": {
    "start": "expo start",
    "reset-project": "node ./scripts/reset-project.js",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "build:web": "npx expo export --platform web && cp -r assets dist/ && sed -i '' 's|<meta http-equiv=\"X-UA-Compatible\" content=\"IE=edge\"/>|<meta http-equiv=\"X-UA-Compatible\" content=\"IE=edge\"/><link rel=\"icon\" type=\"image/png\" href=\"/assets/images/favicon.png\"/><link rel=\"apple-touch-icon\" href=\"/assets/images/icon.png\"/>|g' dist/*.html",
    "deploy:firebase": "npm run build:web && firebase deploy --only hosting",
    "deploy:netlify": "npm run build:web && netlify deploy --dir=dist --prod",
    "deploy:github": "npm run build:web && npx gh-pages -d dist",
    "lint": "expo lint"
  },
  "dependencies": {
    "@expo/metro-runtime": "^6.1.2",
    "@expo/react-native-action-sheet": "^4.1.1",
    "@expo/vector-icons": "^15.0.2",
    "@react-native-async-storage/async-storage": "2.2.0",
    "@react-navigation/bottom-tabs": "^7.3.10",
    "@react-navigation/elements": "^2.3.8",
    "@react-navigation/native": "^7.1.6",
    "axios": "^1.11.0",
    "expo": "^54.0.18",
    "expo-av": "~16.0.7",
    "expo-blur": "~15.0.7",
    "expo-constants": "~18.0.10",
    "expo-dev-client": "~6.0.16",
    "expo-document-picker": "~14.0.7",
    "expo-font": "~14.0.9",
    "expo-haptics": "~15.0.7",
    "expo-image": "~3.0.10",
    "expo-linking": "~8.0.8",
    "expo-router": "~6.0.13",
    "expo-speech": "~14.0.7",
    "expo-splash-screen": "~31.0.10",
    "expo-status-bar": "~3.0.8",
    "expo-symbols": "~1.0.7",
    "expo-system-ui": "~6.0.8",
    "expo-web-browser": "~15.0.8",
    "papaparse": "^5.5.3",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-native": "0.81.5",
    "react-native-actions-sheet": "^0.9.7",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-reanimated": "~4.1.1",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-tts": "^4.1.1",
    "react-native-web": "^0.21.0",
    "react-native-webview": "13.15.0",
    "react-native-worklets": "0.5.1",
    "zustand": "^5.0.6"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@types/papaparse": "^5.3.16",
    "@types/react": "~19.1.10",
    "eslint": "^9.25.0",
    "eslint-config-expo": "~10.0.0",
    "gh-pages": "^6.3.0",
    "typescript": "~5.9.2"
  },
  "private": true
}
